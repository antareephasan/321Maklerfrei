{
  "name": "rocket",
  "version": "2.0.0",
  "description": "Rocket SaaS Boilerplate",
  "private": true,
  "scripts": {
    "tailwind:dev": "tailwindcss build src/assets/css/tailwind.css -o src/assets/css/tailwind.output.css",
    "tailwind:build": "cross-env NODE_ENV=production postcss src/assets/css/tailwind.css -o src/assets/css/tailwind.output.css",
    "prestart": "npm run tailwind:dev",
    "start": "react-scripts --openssl-legacy-provider start",
    "prebuild": "npm run tailwind:build",
    "build": "NODE_OPTIONS= --openssl-legacy-provider react-scripts build",
    "cz": "git-cz",
    "sitemap": "babel-node ./sitemap-builder.js",
    "release": "release-it",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@kapost/react-component-slider": "^1.0.5",
    "@lottiefiles/lottie-player": "^1.5.6",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.6",
    "@mui/styled-engine-sc": "^5.11.0",
    "@react-spring/web": "^9.6.1",
    "@stripe/react-stripe-js": "^1.4.0",
    "@stripe/stripe-js": "^1.13.1",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.5.0",
    "@testing-library/user-event": "7.2.1",
    "@windmill/react-ui": "^0.6.0",
    "axios": "^0.21.1",
    "carbon-components": "^10.56.0",
    "carbon-components-react": "^8.4.0",
    "carbon-icons": "^7.0.7",
    "chart.js": "^3.3.1",
    "classnames": "2.2.6",
    "faker": "4.1.0",
    "firebase": "^10.14.0",
    "flatpickr": "^4.6.13",
    "formik": "^2.2.6",
    "i18next": "^20.4.0",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-http-backend": "^1.3.0",
    "image-to-blob": "^0.2.0",
    "joi": "^17.4.0",
    "js-cookie": "^3.0.0",
    "lightgallery": "^2.7.2",
    "moment": "^2.29.1",
    "piexifjs": "^1.0.4",
    "postcss": "^8.3.0",
    "react": "^16.13.1",
    "react-accessible-accordion": "^3.3.5",
    "react-chartjs-2": "^3.0.0",
    "react-date-picker": "^8.4.0",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.3.4",
    "react-flatpickr": "^3.10.12",
    "react-focus-lock": "2.4.0",
    "react-helmet": "^6.1.0",
    "react-hooks-helper": "^1.6.0",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^11.11.4",
    "react-icons": "^4.3.1",
    "react-image-gallery": "^1.3.0",
    "react-phone-number-input": "^3.2.8",
    "react-player": "^2.9.0",
    "react-router-dom": "5.2.0",
    "react-router-hash-link": "^2.4.3",
    "react-router-sitemap": "^1.2.0",
    "react-scripts": "^4.0.3",
    "react-spring": "^9.6.1",
    "react-stripe-checkout": "^2.6.3",
    "react-tabs": "^3.2.2",
    "react-transition-group": "4.4.1",
    "rocket": "file:",
    "styled-components": "^5.3.6",
    "swiper": "^6.8.4",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@svgr/webpack": "5.4.0",
    "autoprefixer": "^10.2.6",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "7.0.2",
    "cssnano": "^5.0.5",
    "postcss-cli": "^8.3.1",
    "react-axe": "3.5.2",
    "react-error-overlay": "6.0.9",
    "tailwindcss": "^2.1.2"
  },
  "keywords": [
    "windmill",
    "dashboard",
    "admin",
    "tailwind",
    "react"
  ],
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
